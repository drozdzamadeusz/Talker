<script>
    jQuery(function($){
        $.fn.extend({
          checkFloatingPlaceholder: function() {
            return this.each(function() {
                let $this = $(this);
                if($this.val() != ''){
                    $this.closest("div").find("label").addClass("hide");
                }else{
                    $this.closest("div").find("label").removeClass("hide");
                }
            });
          }
        });
        let $inputs = $(".float-label input");
        function checkInputs(){
            $inputs.checkFloatingPlaceholder();
        }
        $inputs.blur(function(){
            $(this).checkFloatingPlaceholder();
        });
        checkInputs();
    });
</script>